{
  "name": "dcent-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -f .env.local -- next dev --turbopack",
    "dev:development": "dotenvx run -f .env.development -- next dev --turbopack",
    "dev:staging": "dotenvx run -f .env.staging -- next dev --turbopack",
    "build": "next build",
    "build:development": "dotenvx run -f .env.development -- next build",
    "build:staging": "dotenvx run -f .env.staging -- next build",
    "build:production": "dotenvx run -f .env.production -- next build",
    "start": "next start",
    "start:development": "dotenvx run -f .env.development -- next start",
    "start:staging": "dotenvx run -f .env.staging -- next start",
    "start:production": "dotenvx run -f .env.production -- next start",
    "lint": "next lint",
    "env:encrypt": "dotenvx encrypt",
    "env:decrypt": "dotenvx decrypt",
    "run:local": "dotenvx run -f .env.local -- yarn dev",
    "run:dev": "dotenvx run -f .env.development -- yarn dev",
    "run:staging": "dotenvx run -f .env.staging -- yarn dev",
    "run:prod": "dotenvx run -f .env.production -- yarn dev"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.45.1",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
